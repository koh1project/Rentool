{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      1,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "eol-last": 0,
    "no-unused-vars": 0,
    "require-jsdoc": 0,
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "camelcase": 1,
    "default-case": 2,
    "no-var": 2
  }
}